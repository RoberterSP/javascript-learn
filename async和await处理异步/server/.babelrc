{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2", "es2015","es2017"
  ],
  "plugins": [
    [
      "component",
      [
        {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
        },
        {
          "libraryName": "mint-ui",
          "style": true
        }
      ]
    ],
    "transform-vue-jsx",
    "transform-runtime"
  ]
}
